{
  "name": "@vtex/cli-plugin-abtest",
  "description": "vtex plugin abtest",
  "version": "0.1.12-beta",
  "bugs": "https://github.com/vtex/cli-plugin-abtest/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/color": "^0.x",
    "@oclif/errors": "^1.2.2",
    "@oclif/config": "^1",
    "numbro": "2.1.0",
    "cli-ux": "^5.2.1",
    "tslib": "^2.0.0",
    "chalk": "^4.1.0",
    "yarn": "^1.21.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/config": "^1.12.11",
    "@oclif/plugin-help": "^3",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/ramda": "^0.27.11",
    "@types/semver": "^7.3.1",
    "eslint": "^7.3.1",
    "eslint-config-oclif": "^3.1",
    "@types/supports-color": "^7.2.0",
    "eslint-config-vtex": "^12.3.0",
    "globby": "^11.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "nodemon-notifier-cli": "https://github.com/Slessi/nodemon-notifier-cli.git",
    "prettier": "^2.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^9.0.0",
    "nock": "^13.0.3",
    "typescript": "^3.8.2",
    "vtex": "^2.121.0-beta.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/build",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/vtex/cli-plugin-abtest",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./build/commands",
    "scope": "vtex",
    "bin": "vtex",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "workspace:abtest": {
        "description": "Creates, finishes, or displays the status of an A/B test. Run 'vtex workspace abtest' to see all 3 subcommands"
      }
    }
  },
  "repository": "vtex/cli-plugin-abtest",
  "scripts": {
    "build-clean": "rm -rf build",
    "build-incremental": "tsc --incremental",
    "build": "yarn build-clean && yarn tsc && OCLIF_COMPILATION=true yarn oclif-dev manifest",
    "ci:prettier-check": "prettier --check --config ./.prettierrc \"./src/**/*.{ts,tsx,js,jsx,json}\"",
    "ci:test": "yarn test --ci",
    "format-lint": "yarn format && yarn lint",
    "format": "prettier --config ./.prettierrc --write \"**/*.{ts,js,json}\"",
    "lint": "eslint ./src --cache --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf build && tsc -b && oclif-dev manifest && oclif-dev readme && cat oclif.manifest.json",
    "prepublishOnly": "bash ./scripts/publishLock.sh",
    "test": "jest --passWithNoTests",
    "version": "oclif-dev readme && git add README.md",
    "watch": "yarn build-clean && yarn nodemon"
  }
}
